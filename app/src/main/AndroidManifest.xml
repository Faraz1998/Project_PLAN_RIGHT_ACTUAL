<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Plan Right"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Reminder" />
        <activity android:name=".Notes_file.AddNote" />
        <activity android:name=".Notes_file.EditNote" />
        <activity android:name=".Notes_file.NoteDetails" />
        <activity android:name=".Notes_file.Notes" />
        <activity android:name=".Home" />
        <activity android:name=".Account" />
        <activity android:name=".Login_Register_files.forgotpass" />
        <activity android:name=".Login_Register_files.register" />
        <activity android:name=".Login_Register_files.Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".ReminderData"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>